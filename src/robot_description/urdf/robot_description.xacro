  <inertial>
    <origin xyz="0.0 5.904170394414034e-18 0.01271329027872195" rpy="0 0 0"/>
    <mass value="1.5115445803512537"/>
    <inertia ixx="0.001005" iyy="0.001005" izz="0.001857" ixy="0.0" iyz="0.0" ixz="0.0"/>
